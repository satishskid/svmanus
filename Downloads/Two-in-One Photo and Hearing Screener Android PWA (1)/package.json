{
  "name": "health-screener-pwa",
  "version": "1.0.0",
  "description": "Comprehensive PWA for vision and hearing screening with batch data management",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "db:push": "drizzle-kit push:mysql",
    "db:generate": "drizzle-kit generate:mysql",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx ./server/_core/migrate.ts",
    "db:seed": "tsx ./server/_core/seed.ts",
    "lighthouse": "lighthouse https://localhost:3000 --view",
    "build:analyze": "vite build --analyze",
    "start": "node ./dist/server.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.6.5",
    "papaparse": "^5.4.1",
    "pdfkit": "^0.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-query": "^3.39.3",
    "recharts": "^2.10.3",
    "sharp": "^0.32.6",
    "sonner": "^1.2.0",
    "superjson": "^2.2.0",
    "tailwindcss": "^4.0.0",
    "uuid": "^9.0.1",
    "wouter": "^3.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "drizzle-kit": "^0.20.8",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "shadcn-ui": "^0.8.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vitest": "^1.0.4"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "health",
    "screening",
    "vision",
    "hearing",
    "pwa",
    "medical",
    "react",
    "typescript"
  ],
  "author": "Health Screener Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/satishskid/svmanus.git"
  },
  "bugs": {
    "url": "https://github.com/satishskid/svmanus/issues"
  },
  "homepage": "https://github.com/satishskid/svmanus#readme"
}
